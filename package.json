{
  "name": "build-champ",
  "version": "1.0.0-beta.2",
  "description": "CLI tool to help manage builds of various projects in a single repository",
  "types": "dist/src/index.d.ts",
  "main": "dist/src/index.js",
  "bin": {
    "build-champ": "dist/src/index.js"
  },
  "scripts": {
    "exec": "tsx src/index.ts",
    "build": "tsc --outDir dist --sourceMap true -p tsconfig.ci.json",
    "test": "jest",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "prepack": "npm run build"
  },
  "repository": "https://github.com/ShadowDog007/build-champ",
  "author": "Michael McKnight (https://github.com/ShadowDog007/build-champ)",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "LICENSE.txt",
    "README.md"
  ],
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "memfs": "^3.4.12",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "glob": "^8.0.3",
    "inversify": "^6.0.1",
    "lodash": "^4.17.21",
    "minimatch": "^5.1.1",
    "p-queue": "^6.6.2",
    "reflect-metadata": "^0.1.13",
    "safe-eval": "^0.4.1",
    "simple-git": "^3.15.1",
    "xml-js": "^1.6.11",
    "yaml": "^2.1.3"
  }
}